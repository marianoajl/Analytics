        {{inputs.floatingLabels (type="text", id="label", "", "", "Nombre","Nombre", "Ingresa tu nombre")}}
        {{inputs.floatingLabels (type="text", id="label", "",  "", "Apellido", "Apellido", "Ingresa tu apellido")}}  
        {{inputs.floatingLabels (type="date", id="label", "",  "", "Fecha", "Fecha", "Ingresa la fecha")}}   
