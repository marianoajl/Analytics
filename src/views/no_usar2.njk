    var checkoutForm = {
        tempData: null,
        setTempData: function (TempData) {
            checkoutForm.tempData = TempData;
        },        
        //updateField es llamar al server a que actualice un campo (por el cambio de selección de otro campo)
        updateField: function (updatingFieldCode, updatingFieldSplitCode, url) {
            var data = {
                'Parameters':
                 {
                     'updatingFieldCode': updatingFieldCode,
                     'updatingFieldSplitCode': updatingFieldSplitCode,
                     'formValues': $('form').serializeArray(),
                     'TempData': checkoutForm.tempData
                 }
            };

            $.ajax({
                type: "POST",
                url: url,
                data: JSON.stringify(data),
                contentType: "application/json",
                dataType: "html",
                success: function (result) {
                    var formFieldId = checkoutForm.getFormFieldId(updatingFieldCode, updatingFieldSplitCode);
                    var jQueryElement = $('#div' + formFieldId);
                    jQueryElement.html(result);
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    console.log(xhr.status);
                    console.log(thrownError);
                }
            });
        },
        //validateField es llamar al server a que valide el dato ingresado en un campo de textbox
        validateField: function (validatingFieldCode, validatingFieldSplitCode, url) {
            var formFieldId = checkoutForm.getFormFieldId(validatingFieldCode, validatingFieldSplitCode);
            var jQueryElement = $('#' + formFieldId);
            var domElement = jQueryElement[0];
            var isValdateRegexValid = domElement.checkValidity();
            if (jQueryElement.attr('pattern')) {
                var reg = new RegExp(jQueryElement.attr("pattern"));
                if (!reg.test(jQueryElement.val())) {
                    //El texto ingresado no es válido según la validación client side. Muestro el error
                    domElement.setCustomValidity(jQueryElement.attr('title'));
                    return;
                }
            }
            //pasó la validación client side. Hago la server side
            $.ajax({
                type: "POST",
                url: url,
                data: $.param({ 'validatingFieldCode': validatingFieldCode }) + '&' + $.param({ 'validatingFieldSplitCode': validatingFieldSplitCode }) + '&' + $('#personalInfo').serialize(),
                success: function (result) {
                    domElement.setCustomValidity(result);
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    console.log(validatingFieldCode);
                    console.log(xhr.status);
                    console.log(thrownError);
                }
            });
        },
        selectComboChanged: function (changedFieldCode, changedFieldSplitCode, externalOperationOnChange, url) {
            var formFieldId = checkoutForm.getFormFieldId(changedFieldCode, changedFieldSplitCode);
            var dependentObjectValue = $('#' + formFieldId + ' option:selected ').attr("data-dependentObject");
            $('#' + formFieldId + '_DependentObject').val(dependentObjectValue);
            gatewayCustomEvents.externalOperationClientSide(changedFieldCode, changedFieldSplitCode);
            if (externalOperationOnChange)
                checkoutForm.externalOperationServerSide(changedFieldCode, changedFieldSplitCode);
        },
        externalOperationServerSide: function (changedFieldCode, changedFieldSplitCode) {
            //$.ajax({
            //    type: "POST",
            //    url: url,
            //    data: $.param({ 'updatingFieldCode': changedFieldCode }) + '&' + $.param({ 'updatingFieldSplitCode': changedFieldSplitCode }) + '&' + $('#personalInfo').serialize(),
            //    success: function (result) {
