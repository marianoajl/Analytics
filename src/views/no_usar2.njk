        externalOperationClientSide: function (changedFieldCode, changedFieldSplitCode) {
            if (changedFieldCode == 'GATEWAYFIELD_INSTALLMENTS') {
                var dependentObject = $("#GATEWAYFIELD_INSTALLMENTS_1 option:selected ").attr("data-dependentObject");
                var isUpdatedVersion = false;
                var dependentInstallments = $("#GATEWAYFIELD_INSTALLMENTS_1 option:selected ").attr("data-installments");;
                if (dependentObject !== undefined || dependentInstallments !== undefined) {
                    var total = "";
                    if (dependentObject == undefined) {
                        isUpdatedVersion = true;
                        total = $("#GATEWAYFIELD_INSTALLMENTS_1_DependentObject").val().split("|")[1];
                        if (total == undefined) {
                            var total = $("#GATEWAYFIELD_INSTALLMENTS_1 option:selected ").text().split('|')[1];
                            if (total.indexOf(':') >= 0) {
                                total = $("#GATEWAYFIELD_INSTALLMENTS_1 option:selected ").text().split('|')[1].split(':')[1]
                            }
                        }
                    }
                    else {
                        total = dependentObject.split("|")[1];
                    }
                    $("#ProductPaymentTotal").text(total);
                    $("#price").text(total);
                    if ($("select#GATEWAYFIELD_INSTALLMENTS_1 option").length > 1) {
                        var txtCuotas = $("#GATEWAYFIELD_INSTALLMENTS_1 option:selected ").text();
                        if (isUpdatedVersion) {
                            txtCuotas = txtCuotas.split("|")[0];
                        }
                        $("#PaymentResumenInstallment").text(txtCuotas);
                        $("#ProductPaymentDetail").text(txtCuotas);
                        $("#PaymentResumenInstallment").show();
                        $("#ProductPaymentDetail").show();
                        if ($("#chkVersion") != undefined && $("#chkVersion").val() == "V2") {
                            var txtCurrencyAmount = "";
                            if (isUpdatedVersion) {
                                txtCurrencyAmount = $("#GATEWAYFIELD_INSTALLMENTS_1 option:selected ").text().split('|')[1];
                            }
                            else {
                                txtCurrencyAmount = $("#GATEWAYFIELD_INSTALLMENTS_1 option:selected").data('dependentobject').split('|')[1];
                            }
                            var x = document.getElementsByClassName("product-payment-total");
                            var i;
                            for (i = 0; i < x.length; i++) {
                                x[i].innerHTML = txtCurrencyAmount;
                            }
                        }
                    }
                }
                else {
                    $("#ProductPaymentTotal").text('');
                    $("#price").text('');
                    $("#PaymentResumenInstallment").hide();
                    $("#ProductPaymentDetail").hide();
                }
            }
        },
        externalOperationServerSideResponse: function (changedFieldCode, changedFieldSplitCode, result) {
            //alert(result);
        }
